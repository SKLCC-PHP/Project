
<div class="pageContent">
	<form method="post" action="{{$smarty.const.__ACTION__&id={{$id}}|url}}" class="pageForm required-validate" onsubmit="return validateCallback(this, dialogAjaxDone);">
		<div class="pageFormContent" layoutH="56">
		<p>第<font color="red">{{$TIME}}</font>批<font color="red">{{$PName}}</font></p>
		<input type = "hidden" name="PID" value = "{{$PID}}">
		<input type = "hidden" name="TIME" value = "{{$TIME}}">
			<div class="divider"></div>
				<dl>
					<dt>开始报名时间</dt>
					<input type="text" class="date" name="begin_add_time" readonly = "readonly" value="{{$list['begin_add_time']}}"/>
					
				</dl>
				<dl>
					<dt>结束报名时间</dt>
					<input type="text" class="date" name="end_add_time" readonly = "readonly" value="{{$list['end_add_time']}}"/>
					
				</dl>

				<dl>
					<dt>开始中期检查时间</dt>
					<input type="text" class="date" name="begin_middle_check_time" readonly = "readonly" value="{{$list['begin_middle_check_time']}}"/>
					
				</dl>
				<dl>
					<dt>结束中期检查时间</dt>
					<input type="text" class="date" name="end_middle_check_time" readonly = "readonly" value="{{$list['end_middle_check_time']}}"/>
					
				</dl>
				<br>

				<dl>
					<dt>开始结题评审时间</dt>
					<input type="text" class="date" name="begin_final_check_time" readonly = "readonly" value="{{$list['begin_final_check_time']}}"/>
					
				</dl>
				<dl>
					<dt>结束结题评审时间</dt>
					<input type="text" class="date" name="end_final_check_time" readonly = "readonly" value="{{$list['end_final_check_time']}}"/>
					
				</dl>

			<div class="divider"></div>
		
		</div>
		<div class="formBar">
			<ul>
				<li><div class="buttonActive"><div class="buttonContent"><button type="submit">保存</button></div></div></li>
				<li>
					<div class="button"><div class="buttonContent"><button type="button" class="close">取消</button></div></div>
				</li>
			</ul>
		</div>
	</form>
</div>
